// CAN(name, id, format)
// Valid formats: BOOL, UINT8, UINT16, UINT32, UINT64, FLOAT
// See WaveSculptor 20 CAN communications protocol datasheet for details
CAN(ID, 0x00, UINT32)
CAN(STATUS, 0x01, UINT16)
CAN(BUS, 0x02, FLOAT)
CAN(VELOCITY, 0x03, FLOAT)
CAN(PHASE_CURRENT, 0x04, FLOAT)
CAN(MOTOR_VOLTAGE_VECTOR, 0x05, FLOAT)
CAN(MOTOR_CURRENT_VECTOR, 0x06, FLOAT)
CAN(MOTOR_BACKEMF, 0x07, FLOAT)
CAN(15_165_RAIL, 0x08, FLOAT)
CAN(25_12_RAIL, 0x09, FLOAT)
CAN(FAN_SPEED, 0x0A, FLOAT)
CAN(SINK_MOTOR_TEMP, 0x0B, FLOAT)
CAN(AIR_IN_CPU_TEMP, 0x0C, FLOAT)
CAN(AIR_OUT_CAP_TEMP, 0x0D, FLOAT)
CAN(ODOMETER_BUS_AMPHOUR, 0x0E, FLOAT)
